        <attr name="cv_chartFillColors" format="reference" />
        <attr name="cv_chartFillPositions" format="reference" />

        <attr name="cv_lineColor" format="color" />
        <attr name="cv_lineWidth" format="dimension" />
        <attr name="cv_lineRoundRadius" format="dimension" />

        <attr name="cv_gridEnabled" format="boolean" />
        <attr name="cv_gridColor" format="color" />
        <attr name="cv_gridLineWidth" format="dimension" />
        <attr name="cv_gridGapWidth" format="dimension" />
        <attr name="cv_gridStepGap" format="dimension" />
        <attr name="cv_gridTextColor" format="color" />
        <attr name="cv_gridLineTextPadding" format="dimension" />
        <attr name="cv_gridTextSize" format="dimension" />

        <attr name="cv_minGridLinesCount" format="integer" />
        <attr name="cv_maxGridLinesCount" format="integer" />

        <attr name="cv_animateOnNewData" format="boolean" />
        <attr name="cv_animationDuration" format="integer" />

    </declare-styleable>
